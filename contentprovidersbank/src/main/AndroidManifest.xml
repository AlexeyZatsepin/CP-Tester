<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="study.network.contentprovidersbank">

    <application
        android:allowBackup="true"
        android:icon="@drawable/bank_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <provider
            android:name=".UsersRelationProvider"
            android:authorities="com.bank.lite.users"
            android:exported="true" />
        <provider
            android:name=".UsersDocProvider"
            android:authorities="com.bank.doc.users"
            android:exported="true" />
        <provider
            android:name=".UsersKVProvider"
            android:authorities="com.bank.kv.users"
            android:exported="true" />
    </application>

</manifest>